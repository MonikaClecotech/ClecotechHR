
<%= render 'view' %>

<%= form_for [@organization, @salary_transaction] do |f|%>

  <%= f.label :user_id %>
  <%= f.text_field :user_id, placeholder: "enter employee id",required: true %>
 
  <%= f.label :salary %>
  <%= f.text_field :salary, placeholder: "enter salary" %>
  
  <%= f.submit 'Save'%>
  
<% end %>